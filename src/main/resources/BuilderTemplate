/**
 * Created by lipeng on 15/7/18.
 */
public class BuilderTemplate {
    private TargetClass obj = new TargetClass();

    private BuilderTemplate() {
    }

    public static BuilderTemplate newBuilder() {
        return new BuilderTemplate();
    }

    public BuilderTemplate fieldWriteMethod(fieldType fieldname){
        obj.fieldWriteMethod(fieldname);
        return this;
    }
}
